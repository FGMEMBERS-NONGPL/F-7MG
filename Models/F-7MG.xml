<!-- **************** Animations ******************* -->

 <!-- Landing Gear and Doors -->
 <animation>
  <type>select</type>
  <object-name>LMainGear</object-name>
  <object-name>RMainGear</object-name>
  <object-name>LMainTire</object-name>
  <object-name>RMainTire</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0.05</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>LMainGearDoorInner</object-name>
  <object-name>RMainGearDoorInner</object-name>
  <object-name>NoseGearDoorLeft</object-name>
  <object-name>NoseGearDoorRight</object-name>
  <object-name>NoseGearStrut</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>MainGearWellCover</object-name>
  <object-name>NoseWellCover</object-name>
  <condition>
   <equals>
    <property>gear/gear[0]/position-norm</property>
    <value>0</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>NoseGearStrutLower</object-name>
  <object-name>NoseGearTire</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0.1</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NoseGearStrut</object-name>
  <property>gear/gear[0]/position-norm</property>
  <factor>120</factor>
  <offset-deg>-120</offset-deg>
  <center>
   <x-m>-4.208</x-m>
   <y-m>0</y-m>
   <z-m>-0.545</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NoseGearStrutLower</object-name>
  <object-name>NoseGearTire</object-name>
  <property>gear/gear[0]/position-norm</property>
  <factor>120</factor>
  <offset-deg>-120</offset-deg>
  <center>
   <x-m>-4.208</x-m>
   <y-m>0</y-m>
   <z-m>-0.545</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NoseGearTire</object-name>
  <object-name>NoseGearStrutLower</object-name>
  <property>sim/multiplay/generic/float[2]</property>
  <factor>60</factor>
  <center>
   <x-m>-4.267</x-m>
   <y-m>0</y-m>
   <z-m>-1.07</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>-1</z>
  </axis>
 </animation>

 <animation>
  <type>spin</type>
  <object-name>NoseGearTire</object-name>
  <property>fdm/jsbsim/gear/unit[0]/wheel-speed-fps</property>
  <factor>77.6</factor>
  <center>
   <x-m>-4.052</x-m>
   <y-m>0</y-m>
   <z-m>-1.178</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>-1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>NoseGearDoorLeft</object-name>
  <property>gear/gear[0]/position-norm</property>
  <interpolation>
    <entry><ind>0</ind><dep>90</dep></entry>
    <entry><ind>0.1</ind><dep>0</dep></entry>
